{
  "name": "DiscoTegaki",
  "version": "1.4.4",
  "manifest_version": 3,
  "description": "Add a small canvas to Discord and other social media sites.",
  "content_scripts": [
    {
      "matches": [
        "https://discord.com/*",
        "https://mebuki.moe/*"
      ],
      "css": ["style.css"],
      "js": ["main.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  }
}