{
  "name": "DiscoTegaki",
  "version": "1.3.0",
  "manifest_version": 3,
  "description": "Add a small canvas to Discord.",
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "css": ["style.css"],
      "js": ["main.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://discord.com/*"],
        "resources": ["asset/*"]
    }
  ],
  "icons": {
    "16": "asset/icon_16.png",
    "48": "asset/icon_48.png",
    "128": "asset/icon_128.png"
  }
}